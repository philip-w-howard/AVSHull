<Application x:Class="AVSHull.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AVSHull"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <local:GCodeParameters x:Key="GCodeSetup"
                PlungeSpeed="5"
                CutSpeed = "30"
                CutterDiameter = "0.25"
                OffsetInside = "false"
                MaterialThickness = "0.25"
                CutthroughDepth = "0.05"
                DepthPerCut = "0.15"
                NumTabs = "4"
                TabDepth = "0.0625"
                TabWidth = "0.375"
                Origin = "0" />
        <local:LayoutSetupData x:Key="LayoutSetup" sheetWidth="96" sheetHeight="48" numSheetsHorizontal="3" numSheetsVertical="2"/>
        <local:PanelSplitSetupValues x:Key="SplitSetup" Start="0" NumTongues="3" Depth="3" />

    </Application.Resources>
</Application>
